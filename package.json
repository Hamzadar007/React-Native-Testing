{
  "name": "rn_testing_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint:fix": "eslint src --fix",
    "format": "prettier --check src",
    "format:fix": "prettier --write src",
    "start": "react-native start",
    "test": "jest",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android-debug": "cd android && ./gradlew assembleDebug",
    "build:apk": "./scripts/build-apk.sh",
    "detox:android_build": "detox build --configuration android.emu.debug",
    "detox:android_test": "detox test --configuration android.emu.debug",
    "detox:ios_build": "detox build --configuration ios.sim.debug",
    "detox:ios_test": "detox test --configuration ios.sim.debug",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.11.0",
    "lottie-react-native": "^7.3.1",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-animated-otp-input": "^0.1.8",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-heroicons": "^4.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.3.0",
    "react-native-pager-view": "^6.9.1",
    "react-native-reanimated": "^4.0.2",
    "react-native-reanimated-carousel": "^4.0.3",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.13.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-svg": "^15.12.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-swiper": "^1.6.0",
    "react-native-worklets": "^0.4.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-jest": "^30.0.5",
    "detox": "^20.40.2",
    "eslint": "^8.19.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
